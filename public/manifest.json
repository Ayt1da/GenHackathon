{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0.0",
  "description": "This is my first Chrome extension.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html"
  },

  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}